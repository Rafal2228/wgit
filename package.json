{
  "name": "wgit",
  "version": "0.1.3",
  "preferGlobal": true,
  "description": "Just a cli for git",
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Mc01/wgit.git"
  },
  "author": "Mc",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Mc01/wgit/issues"
  },
  "homepage": "https://github.com/Mc01/wgit#readme",
  "dependencies": {
    "chalk": "^1.1.1",
    "cmd-exec": "^0.1.0",
    "commander": "^2.9.0",
    "findup-sync": "^0.4.0",
    "path": "^0.12.7"
  },
  "devDependencies": {
    "ava": "^0.14.0",
    "babel-cli": "^6.7.5",
    "babel-preset-es2015": "^6.6.0",
    "coveralls": "^2.11.9",
    "jscs": "^3.0.3",
    "jshint": "^2.9.1",
    "nyc": "^6.4.0",
    "precommit-hook": "^3.0.0"
  },
  "scripts": {
    "develop": "babel -d lib/ src/ -w",
    "compile": "babel -d lib/ src/",
    "prestart": "npm run compile",
    "start": "node lib/index.js",
    "test": "npm run compile && nyc ava",
    "test-debug": "npm run compile && nyc ava --serial --tap",
    "test-watch": "npm run compile && nyc ava --watch",
    "report": "nyc report --reporter=html",
    "lint": "jshint ./src && jscs ./src && jshint ./test && jscs ./test",
    "validate": "npm ls",
    "wgit": "node lib/index.js"
  },
  "bin": {
    "wgit": "lib/index.js"
  },
  "engines": {
    "node": ">= 0.10.x"
  },
  "ava": {
    "files": [
      "test/*.test.js"
    ],
    "failFast": true
  },
  "pre-commit": [
    "lint",
    "validate"
  ]
}
